{
  "name": "tevet-troc-client",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "includedScripts": [],
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "./src",
  "tags": [],
  "targets": {
    "prepare-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["node tools/generate-env.mjs"]
      }
    },
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "dependsOn": ["prepare-env"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tevet-troc-client",
        "index": "./src/index.html",
        "main": "./src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "public"
          },
          {
            "glob": "**/*",
            "input": "src/assets/icons/svgs",
            "output": "assets/icons/svgs"
          }
        ],
        "styles": ["./src/styles.scss"],
        "scripts": ["./src/scripts/process-env.js"]
      },
      "configurations": {
        "production": {
          "baseHref": "/tevet-troc-client/",
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "5kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "build-github": {
      "executor": "@angular-devkit/build-angular:browser",
      "dependsOn": ["prepare-env"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tevet-troc-client",
        "index": "./src/index.html",
        "main": "./src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "public"
          },
          {
            "glob": "**/*",
            "input": "src/assets/icons/svgs",
            "output": "assets/icons/svgs"
          }
        ],
        "styles": ["./src/styles.scss"],
        "scripts": ["./src/scripts/process-env.js"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "dependsOn": ["prepare-env"],
      "configurations": {
        "production": {
          "buildTarget": "tevet-troc-client:build:production"
        },
        "development": {
          "buildTarget": "tevet-troc-client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "tevet-troc-client:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["./src"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectName}"],
      "options": {
        "jestConfig": "jest.config.app.ts",
        "tsConfig": "tsconfig.spec.json"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "tevet-troc-client:build",
        "port": 4200,
        "staticFilePath": "dist/tevet-troc-client",
        "spa": true
      }
    }
  }
}
